{
  "presets": [
    ["@stellar-apps/react", {"removePropTypes": false}],
    ["@emotion/css-prop", {"sourceMap": false, "hoist": true, "autoLabel":  false}]
  ],
  "plugins": [
    "polished"
  ],
  "env": {
    "cjs": {
      "presets": [
        [
          "@stellar-apps/es",
          {
            "env": {
              "useBuiltIns": false,
              "targets": {"browsers": ">5% in US"}
            }
          }
        ]
      ]
    },
    "es": {
      "presets": [
        [
          "@stellar-apps/es",
          {
            "env": {
              "useBuiltIns": false,
              "modules": false,
              "targets": {"browsers": ">5% in US"}
            },
            "runtime": {"useESModules": true}
          }
        ]
      ]
    }
  }
}
