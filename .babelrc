{
  "presets": [
    ["@inst-app/react", {"removePropTypes": false}]
  ],
  "plugins": [
    ["emotion", {"sourceMap": false, "hoist": true}],
    "polished"
  ],
  "env": {
    "cjs": {
      "presets": [
        [
          "@inst-app/esx",
          {
            "env": {
              "targets": {"chrome": "41", "ie": "10"}
            },
            "runtime": {"corejs": 2}
          }
        ]
      ]
    },
    "es": {
      "presets": [
        [
          "@inst-app/esx",
          {
            "env": {
              "modules": false,
              "targets": {"chrome": "41", "ie": "10"}
            },
            "runtime": {"corejs": 2}
          }
        ]
      ]
    }
  }
}
