{
  "presets": [
    ["@stellar-apps/react", {"removePropTypes": false}]
  ],
  "plugins": [
    "polished",
    ["emotion", {"sourceMap": false, "hoist": true, "autoLabel":  false}]
  ],
  "env": {
    "cjs": {
      "presets": [
        [
          "@stellar-apps/es",
          {
            "env": {
              "targets": {"chrome": "41", "ie": "10"}
            },
            "runtime": {"corejs": 2}
          }
        ]
      ]
    },
    "es": {
      "presets": [
        [
          "@stellar-apps/es",
          {
            "env": {
              "modules": false,
              "targets": {"chrome": "41", "ie": "10"}
            },
            "runtime": {"corejs": 2}
          }
        ]
      ]
    }
  }
}
