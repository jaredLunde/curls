{
  "env": {
    "production": {
      "presets": [
        "@babel/preset-react",
        [
          "@babel/preset-env", {
            "debug": true,
            "modules": false,
            "useBuiltIns": "usage",
            "targets": {
              "browsers": "defaults"
            }
          }
        ],
        "@babel/preset-stage-0"
      ],
      "plugins": [
        ["emotion", { "hoist": true }],
        "transform-react-pure-components",
        "@babel/plugin-transform-react-constant-elements",
        "transform-react-remove-prop-types"
      ]
    },
    "commonjs": {
      "presets": [
        "@babel/preset-react",
        [
          "@babel/preset-env", {
            "debug": true,
            "loose": true,
            "modules": "commonjs",
            "useBuiltIns": "usage"
          }
        ],
        "@babel/preset-stage-0"
      ],
      "plugins": [
        ["emotion", { "hoist": true }],
        "@babel/plugin-transform-runtime",
        "transform-react-pure-components",
        "@babel/plugin-transform-react-constant-elements",
        "transform-react-remove-prop-types"
      ]
    },
    "es": {
      "presets": [
        "@babel/preset-react",
        [
          "@babel/preset-env", {
            "debug": true,
            "modules": false,
            "useBuiltIns": "usage"
          }
        ],
        "@babel/preset-stage-0"
      ],
      "plugins": [
        ["emotion", { "hoist": true }],
        "@babel/plugin-transform-runtime",
        "transform-react-pure-components",
        "@babel/plugin-transform-react-constant-elements",
        "transform-react-remove-prop-types"
      ]
    },
    "development": {
      "presets": [
        "@babel/preset-react",
        [
          "@babel/preset-env", {
            "debug": false,
            "modules": false,
            "useBuiltIns": "usage",
            "targets": {
              "browsers": ["chrome >= 56"]
            }
          }
        ],
        "@babel/preset-stage-0"
      ],
      "plugins": [
        ["emotion", { "sourceMap": true, "autoLabel": true }],
        "@babel/plugin-transform-runtime",
        "transform-react-pure-components",
        "@babel/plugin-transform-react-constant-elements",
        "transform-react-remove-prop-types"
      ]
    },
    "cli": {
      "presets": [
        "@babel/preset-react",
        [
          "@babel/preset-env", {
            "debug": true,
            "modules": false,
            "useBuiltIns": "usage"
          }
        ],
        "@babel/preset-stage-0"
      ],
      "plugins": [
        ["emotion", { "hoist": true }],
        "@babel/plugin-transform-runtime",
        "transform-react-pure-components",
        "@babel/plugin-transform-react-constant-elements",
        "transform-react-remove-prop-types"
      ]
    }
  }
}
