{
  "presets": [
    "@babel/preset-react",
    [
      "@lunde/es",
      {
        "env": {
          "targets": {
            "node": "8"
          }
        },
        "typescript": false
      }
    ],
    ["@emotion/css-prop", {"sourceMap": false, "hoist": true, "autoLabel":  false}]
  ],
  "plugins": [
    "polished"
  ],
  "env": {
    "es": {
      "presets": [
        [
          "@lunde/es",
          {
            "env": {
              "modules": false,
              "targets": {
                "browsers": "> 2%"
              }
            },
            "typescript": false
          }
        ]
      ]
    }
  }
}
