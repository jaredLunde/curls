{
  "name": "curls",
  "version": "1.0.0",
  "main": "commonjs/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "esnext": "es/index.js",
  "repository": "https://github.com/jaredlunde/curls",
  "author": "Jared Lunde",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.35",
    "@babel/core": "^7.0.0-beta.35",
