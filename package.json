{
  "name": "styled-curls",
  "version": "1.0.0",
  "main": "es/index.js",
  "repository": "https://github.com/jaredlunde/styled-curls",
  "author": "Jared Lunde",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.35",
    "@babel/core": "^7.0.0-beta.35",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.35",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-beta.35",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.36",
    "@babel/polyfill": "^7.0.0-beta.35",
    "@babel/preset-env": "^7.0.0-beta.35",
    "@babel/preset-react": "^7.0.0-beta.36",
    "@babel/preset-stage-0": "^7.0.0-beta.36",
    "@babel/runtime": "^7.0.0-beta.36",
    "babel-plugin-emotion": "^8.0.12",
    "babel-plugin-transform-react-pure-components": "^3.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "cross-env": "^5.1.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rollup": "^0.53.0",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "dependencies": {
    "emotion": "^9.0.0-1",
    "immutable": "^3.8.2",
    "is-mergeable-object": "^1.1.0",
    "lru-memoize-map": "^0.1.0",
    "polished": "^1.9.0",
    "prop-types": "^15.6.0",
    "react-emotion": "^8.0.12"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node server.js",
    "build": "yarn run build:commonjs && yarn run build:es && yarn run build:dist",
    "build:commonjs": "rm -rf commonjs && cross-env NODE_ENV=commonjs babel ./src --out-dir commonjs",
    "build:es": "rm -rf es && cross-env NODE_ENV=es babel ./src --out-dir es",
    "build:dist:js": "cross-env BABEL_ENV=production NODE_ENV=production rollup -c -i src/index.js -o dist/curls.js",
    "build:dist": "rm -rf dist && cross-env NODE_ENV=production yarn build:dist:js",
    "node": "cross-env NODE_ENV=cli babel-node",
    "watch:commonjs": "cross-env NODE_ENV=commonjs babel ./src -w --out-dir commonjs",
    "watch:es": "cross-env NODE_ENV=es babel ./src -w --out-dir es"
  }
}
